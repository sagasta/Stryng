<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>José Manuel Sagasta</Authors>
		<PackageId>Stryng</PackageId>
		<Version>0.1.0</Version>
		<Description>A set of dev-friendly string validation and generation utilities for .NET</Description>
		<PackageTags>string;validation;generation;utilities;dev</PackageTags>
		<RepositoryUrl>https://github.com/sagasta/Stryng</RepositoryUrl>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<LangVersion>12.0</LangVersion>
	</PropertyGroup>

</Project>

